<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="321.0" prefWidth="375.0" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="cz.unicode.gastro.FXMLController">
    <children>
        <Button fx:id="button" layoutX="14.0" layoutY="69.0" onAction="#handleButtonAction" text="Click Me!" />
        <Label fx:id="label" layoutX="126" layoutY="120" minHeight="16" minWidth="69" />
      <RadioButton fx:id="radioButtonServer" layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#handleCheckBoxServer" selected="true" text="Server" />
      <RadioButton fx:id="radioButtonClient" layoutX="14.0" layoutY="39.0" mnemonicParsing="false" onAction="#handleCheckBoxClient" text="Client" />
    </children>
</AnchorPane>
